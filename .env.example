# Environment Configuration Example
# Copy this file to .env and fill in your actual values

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api
EXPO_PUBLIC_GROQ_API_KEY=your_groq_api_key_here
EXPO_PUBLIC_GROQ_API_URL=https://api.groq.com/openai/v1

# Health Data Integration
EXPO_PUBLIC_MYFITNESSPAL_CLIENT_ID=your_myfitnesspal_client_id
EXPO_PUBLIC_FITBIT_CLIENT_ID=your_fitbit_client_id
EXPO_PUBLIC_OURA_CLIENT_ID=your_oura_client_id

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here
EXPO_PUBLIC_ANALYTICS_ID=your_analytics_id_here

# Feature Flags
EXPO_PUBLIC_ENABLE_BIOMETRIC_AUTH=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_ENABLE_DEBUG_LOGGING=true